# Parameters file

# Geometry and mesh
set dim = 2
set L = 1 x 1
set N = 100 x 100

# Material parameters
set mobility = 0.1
set kappa = 0.0005  # gradient energy parameter

# Neumann boundary value
set influx1 = 0.
set influx2 = 0.

# Time stepping
set dtVal = 0.1
set totalTime = 20

# Restart
set RESTART_TIME = 0
set RESTART_IT = 0

# Output
set skipOutput = 1

# Splines
set polyOrder = 2
set globalContinuity = 1
